# Allow hal_lineage_touch_default to read and write to proc_touchpanel
allow hal_lineage_touch_default proc_touchpanel:dir search;
allow hal_lineage_touch_default proc_touchpanel:file rw_file_perms;

# Allow hal_lineage_touch_default to default_android_hwservice
allow hal_lineage_touch_default default_android_hwservice:hwservice_manager find;